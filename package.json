    "build": "html-minifier-next --config-file=html-minifier-next.json --input-dir=. --output-dir=dist",
    "clean": "IF EXIST dist rmdir /s /q dist",
    "copysvg": "copy *.svg dist",
    "copysvgpdf": "robocopy ./ ./dist *.svg *.pdf /njh /njs",
    "buildfull": "npm run clean && npm run build && npm run copysvgpdf"
  }
